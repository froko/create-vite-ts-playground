{
  "name": "vue-tailwind-playwright-histoire",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "format": "prettier . --write && eslint . --cache --fix",
    "start": "vite --port 3000",
    "build": "vue-tsc && vite build",
    "preview": "vite preview --port 3000",
    "playwright": "playwright test",
    "playwright:ct": "playwright test -c playwright-ct.config.ts",
    "playwright:report": "playwright show-report",
    "histoire": "histoire dev"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@histoire/plugin-vue": "^0.16.4",
    "@playwright/experimental-ct-vue": "^1.37.0",
    "@playwright/test": "^1.37.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "histoire": "^0.16.4",
    "lint-staged": "^14.0.0",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vue-eslint-parser": "^9.3.1",
    "vue-loader": "^17.2.2",
    "vue-tsc": "^1.8.8"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint --cache --fix",
    "*.{html,css,scss,json,md}": "prettier --write"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  }
}
