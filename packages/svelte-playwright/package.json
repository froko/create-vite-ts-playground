{
  "name": "svelte-playwright",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "format": "prettier . --write && eslint . --cache --fix",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "start": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview --port 3000",
    "playwright": "playwright test",
    "playwright:ct": "playwright test -c playwright-ct.config.ts",
    "playwright:report": "playwright show-report"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "fa-svelte": "^3.1.0"
  },
  "devDependencies": {
    "@playwright/experimental-ct-svelte": "^1.37.0",
    "@playwright/test": "^1.37.0",
    "@sveltejs/vite-plugin-svelte": "^2.4.5",
    "@tsconfig/svelte": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-svelte": "^2.32.4",
    "lint-staged": "^14.0.0",
    "prettier": "^3.0.2",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.0",
    "svelte-eslint-parser": "^0.32.2",
    "svelte-loader": "^3.1.9",
    "svelte-preprocess": "^5.0.4",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,svelte}": [
      "svelte-check",
      "eslint --cache --fix"
    ],
    "*.{html,css,scss,json,md}": "prettier --write"
  }
}
