{
  "name": "vue-playwright-histoire",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "format": "prettier . --write && eslint . --cache --fix",
    "start": "vite --port 3000",
    "build": "vue-tsc && vite build",
    "preview": "vite preview --port 3000",
    "playwright": "playwright test",
    "playwright:ct": "playwright test -c playwright-ct.config.ts",
    "playwright:report": "playwright show-report",
    "histoire": "histoire dev"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@histoire/plugin-vue": "^0.16.1",
    "@playwright/experimental-ct-vue": "^1.35.1",
    "@playwright/test": "^1.35.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.15.0",
    "histoire": "^0.16.2",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vue-eslint-parser": "^9.3.1",
    "vue-loader": "^17.2.2",
    "vue-tsc": "^1.8.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint --cache --fix",
    "*.{html,css,scss,json,md}": "prettier --write"
  }
}
