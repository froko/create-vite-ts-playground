{
  "name": "lit-cypress-storybook",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "format": "prettier . --write && eslint . --cache --fix",
    "start": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview --port 3000",
    "cypress": "run-p start cypress:open",
    "cypress:ci": "npm run build && run-p -rs preview cypress:run",
    "cypress:open": "cypress open --e2e --browser electron",
    "cypress:run": "cypress run --spec 'cypress/e2e/*.cy.ts'",
    "storybook": "storybook dev -p 6006"
  },
  "dependencies": {
    "fa-icons": "^0.2.0",
    "lit": "^2.7.6"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.1.1",
    "@storybook/web-components": "^7.1.1",
    "@storybook/web-components-vite": "^7.1.1",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "cypress": "^12.17.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "storybook": "^7.1.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vite-plugin-static-copy": "^0.17.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{html,css,scss,json,md}": "prettier --write"
  }
}
