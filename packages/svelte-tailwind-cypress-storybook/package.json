{
  "name": "svelte-tailwind-cypress-storybook",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "format": "prettier . --write && eslint . --cache --fix",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "start": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview --port 3000",
    "cypress": "run-p start cypress:open",
    "cypress:ci": "npm run build && run-p -rs preview cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --spec 'cypress/e2e/*.cy.ts'",
    "storybook": "storybook dev -p 6006"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "fa-svelte": "^3.1.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.1.1",
    "@storybook/addon-essentials": "^7.1.1",
    "@storybook/addon-interactions": "^7.1.1",
    "@storybook/jest": "^0.1.0",
    "@storybook/svelte": "^7.1.1",
    "@storybook/svelte-vite": "^7.1.1",
    "@storybook/testing-library": "^0.2.0",
    "@sveltejs/vite-plugin-svelte": "^2.4.3",
    "@tsconfig/svelte": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.17.2",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-svelte": "^2.32.4",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.1.1",
    "svelte": "^4.1.1",
    "svelte-check": "^3.4.6",
    "svelte-eslint-parser": "^0.32.2",
    "svelte-loader": "^3.1.9",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.7"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,svelte}": [
      "svelte-check",
      "eslint --cache --fix"
    ],
    "*.{html,css,scss,json,md}": "prettier --write"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  }
}
